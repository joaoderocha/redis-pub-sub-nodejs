{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "max-len": ["error", { "code": 96, "tabWidth": 4 }]
    }
}
